div.chat
    div.chat__container
        div.header
            h2 You are logged in as %username%
        div.chat__box
            each message in messages
                div.message-box.left-img
                    div.picture
                        img(src= message.avatar)
                        span.time 21:30
                    div.message
                        span=message.name
                        p=message.text
